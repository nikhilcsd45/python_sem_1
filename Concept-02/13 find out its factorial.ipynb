   ],
   "source": [
    "def find_fact(num1):\n",
    "    product=1\n",
    "    for x in range(2,num1+1):\n",
    "        product = product*x\n",
    "    return \"the factorial of\",num1,\"is\",product\n",
    "find_fact(5)"
   ]
  }
