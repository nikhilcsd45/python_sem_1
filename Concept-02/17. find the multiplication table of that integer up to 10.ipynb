   ],
   "source": [
    "def find_table(num1):\n",
    "    p=1\n",
    "    for x in range(1,10+1):\n",
    "        p= num1*(x)#p=product\n",
    "        num2=p\n",
    "        print(num1 ,\"x\", x ,\"=\", num2)\n",
    "find_table(40)"
   ]
  }
